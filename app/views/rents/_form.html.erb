<%= form_with model: rent do |form| %>
  <div>
    <%= form.label :movie_id, class: "form-label" %>
    <%= form.text_field :movie_id, class: "form-control" %>
    <% rent.errors.full_messages_for(:movie).each do |message| %>
      <div class="alert alert-danger p-2"><%= message %></div>
    <% end %>
  </div>
  <div>
    <%= form.label :user_id, class: "form-label" %>
    <%= form.text_field :user_id, class: "form-control" %>
    <% rent.errors.full_messages_for(:user).each do |message| %>
      <div class="alert alert-danger p-2"><%= message %></div>
    <% end %>
  </div>
  <div>
    <%= form.submit class: "btn btn-dark mt-2" %>
  </div>
<% end %>