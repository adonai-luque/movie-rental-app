<h1>Rents</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Movie ID</th>
      <th scope="col">User ID</th>
    </tr>
  </thead>
  <tbody>
    <% @rents.each do |rent| %>
      <tr>
        <th scope="row"><%= rent.id %></th>
        <td><%= rent.movie_id %></td>
        <td><%= rent.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
