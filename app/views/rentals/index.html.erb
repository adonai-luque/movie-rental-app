<h1>Rentals</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Movie ID</th>
      <th scope="col">User ID</th>
    </tr>
  </thead>
  <tbody>
    <% @rentals.each do |rental| %>
      <tr>
        <th scope="row"><%= rental.id %></th>
        <td><%= rental.movie_id %></td>
        <td><%= rental.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
